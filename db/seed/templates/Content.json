{
  "http://disco-network.org/resource/Content": {
    "$": {
      "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "disco": "http://disco-network.org/resource/",
        "Content": "disco:Content",
        "Id": {
          "@id": "disco:id",
          "@type": "xsd:string"
        },
        "Key": {
          "@id": "disco:key",
          "@type": "xsd:string"
        },
        "Modified": {
          "@id": "disco:modified",
          "@type": "xsd:string"
        },
        "Title": {
          "@id": "disco:title",
          "@type": "xsd:string"
        },
        "Text": {
          "@id": "disco:text",
          "@type": "xsd:string"
        },
        "Culture": {
          "@id": "Content:#culture",
          "@type": "@id"
        }
      },
      "@remove": [
        "odata.metadata",
        "value"
      ],
      "@transform": {
        "@graph": [
          {
            "f:valueof": "value"
          }
        ]
      }
    },
    "$.value.*": {
      "@type": "Content",
      "@id": [
        {
          "f:valueof": "Id"
        },
        {
          "f:prefix": "disco:Content/"
        }
      ],
      "@transform": {
        "Culture": [
          {
            "f:valueof": "CultureId"
          },
          {
            "f:prefix": "disco:Cultures/"
          }
        ]
      },
      "@remove": [
        "CultureId"
      ]
    }
  }
}