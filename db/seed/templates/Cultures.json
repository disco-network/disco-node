{
  "http://disco-network.org/resource/Cultures": {
    "$": {
      "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "disco": "http://disco-network.org/resource/",
        "Culture": "disco:Culture",
        "Id": {
          "@id": "disco:id",
          "@type": "xsd:string"
        },
        "Key": {
          "@id": "disco:key",
          "@type": "xsd:string"
        },
        "Modified": {
          "@id": "disco:modified",
          "@type": "xsd:dateTime"
        },
        "Code": {
          "@id": "disco:code",
          "@type": "xsd:string"
        },
        "Name": {
          "@id": "disco:name",
          "@type": "xsd:string"
        }
      },
      "@remove": [
        "odata.metadata",
        "value"
      ],
      "@transform": {
        "@graph": [
          {
            "f:valueof": "value"
          }
        ]
      }
    },
    "$.value.*": {
      "@type": "Culture",
      "@id": [
        {
          "f:valueof": "Id"
        },
        {
          "f:prefix": "disco:Cultures/"
        }
      ]
    }
  }
}