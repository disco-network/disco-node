{
  "http://disco-network.org/resource/Descriptors": {
    "$": {
      "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "disco": "http://disco-network.org/resource/",
        "Descriptor": "disco:Descriptor",
        "Id": {
          "@id": "disco:id",
          "@type": "xsd:string"
        },
        "Key": {
          "@id": "disco:key",
          "@type": "xsd:string"
        },
        "Modified": {
          "@id": "disco:modified",
          "@type": "xsd:string"
        },
        "Name": {
          "@id": "disco:name",
          "@type": "xsd:string"
        },
        "Description": {
          "@id": "disco:description",
          "@type": "xsd:string"
        }
      },
      "@remove": [
        "odata.metadata",
        "value"
      ],
      "@transform": {
        "@graph": [
          {
            "f:valueof": "value"
          }
        ]
      }
    },
    "$.value.*": {
      "@type": "Descriptor",
      "@id": [
        {
          "f:valueof": "Id"
        },
        {
          "f:prefix": "disco:Descriptors/"
        }
      ],
      "@transform": {
        "Culture": [
          {
            "f:valueof": "CultureId"
          },
          {
            "f:prefix": "disco:Cultures/"
          }
        ]
      }
    }
  }
}