{
  "http://disco-network.org/resource/PostTypes": {
    "$": {
      "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "disco": "http://disco-network.org/resource/",
        "PostType": "disco:PostType",
        "Id": {
          "@id": "disco:id",
          "@type": "xsd:string"
        },
        "Key": {
          "@id": "disco:key",
          "@type": "xsd:string"
        },
        "Modified": {
          "@id": "disco:modified",
          "@type": "xsd:string"
        },
        "Description": {
          "@id": "disco:description",
          "@type": "@id"
        }
      },
      "@remove": [
        "odata.metadata",
        "value"
      ],
      "@transform": {
        "@graph": [
          {
            "f:valueof": "value"
          }
        ]
      }
    },
    "$.value.*": {
      "@type": "PostType",
      "@id": [
        {
          "f:valueof": "Id"
        },
        {
          "f:prefix": "disco:PostTypes/"
        }
      ],
      "@transform": {
        "Description": [
          {
            "f:valueof": "DescriptionId"
          },
          {
            "f:prefix": "disco:Descriptors/"
          }
        ]
      },
      "@remove": [
        "DescriptionId"
      ]
    }
  }
}