{
  "http://disco-network.org/resource/Posts": {
    "$": {
      "@context": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "disco": "http://disco-network.org/resource/",
        "Post": "disco:Post",
        "Id": {
          "@id": "disco:id",
          "@type": "xsd:string"
        },
        "Key": {
          "@id": "disco:key",
          "@type": "xsd:string"
        },
        "Modified": {
          "@id": "disco:modified",
          "@type": "xsd:string"
        },
        "PostType": {
          "@id": "disco:postType",
          "@type": "@id"
        },
        "Content": {
          "@id": "disco:content",
          "@type": "@id"
        },
        "Tags": {
          "@id": "disco:tag",
          "@type": "@id"
        },
        "Ratings": {
          "@id": "disco:rating",
          "@type": "@id"
        }
      },
      "@remove": [
        "odata.metadata",
        "value"
      ],
      "@transform": {
        "@graph": [
          {
            "f:valueof": "value"
          }
        ]
      }
    },
    "$.value.*": {
      "@type": "Post",
      "@id": [
        {
          "f:valueof": "Id"
        },
        {
          "f:prefix": "disco:Posts/"
        }
      ],
      "@transform": {
        "PostType": [
          {
            "f:valueof": "PostTypeId"
          },
          {
            "f:prefix": "disco:PostTypes/"
          }
        ],
        "Content": [
          {
            "f:valueof": "ContentId"
          },
          {
            "f:prefix": "disco:Content/"
          }
        ]
      },
      "@remove": [
        "PostTypeId",
        "ContentId",
        "RefersTo",
        "ReferredFrom"
      ]
    }
  }
}